   , "verenigbaarMet" : verenigbaarMet
   , "verzamelDoel" : verzamelDoel~
   , "svcVerzamelDoel" : svcVerzamelDoel~
   , "verwerkingsDoel" : verwerkingsDoel~
   , "svcVerwerkingsDoel" : svcVerwerkingsDoel~
   ]
