SERVICE BasisgegevensUC001 : I[Procedur] -- UC001 Registreren basisgegevens procedure
{- Het aanmaken van een nieuwe procedure middels het registreren van de basisgegevens van een centraal binnengekomen beroepschrift of verzoekschrift om voorlopige voorziening en het automatisch (geen samenhangende procedure aangegeven) of handmatig (wel samenhangende procedure aangegeven) voorlopig/definitief toewijzen van de nieuwe procedure aan een behandelend gerecht.
-}
   = [ eiser : eiser~;door~/\inzake~
        = [ partij : door
          , gemachtigde : gemachtigde~
          ]
     , gedaagde : gedaagde~;door~/\inzake~
        = [ partij : door
          , gemachtigde : gemachtigde~
          ]
     , gevoegde : gevoegde~;door~/\inzake~
        = [ partij : door
          , gemachtigde : gemachtigde~
          ]
     , rechtsgebied : rechtsgebied
     , proceduresoort : proceduresoort
     , bevoegd : bevoegd
     , "zorgdrager voor dossier" : zorgdrager             -- Orgaan
     , dossierstukken : zaaksdossier~                     -- [Document]
        = [ Document : I[Document]
          , type : type
          ]
     , cluster : cluster
        = [ naam  : I[Cluster]
          , grond : grond ]
     , machtigingen : inzake~
        = [ machtiging : I[Machtiging]
          , partij : door
          , gemachtigde : gemachtigde~
          ]
     ]

SERVICE Procedure : I[Procedur]
   = [ eiser : eiser~          -- Persoon
     , "gemachtigde voor eiser" : (eiser~;door~/\inzake~);gemachtigde~
     , gedaagde : gedaagde~          -- Persoon
     , "gemachtigde voor gedaagde" : (gedaagde~;door~/\inzake~);gemachtigde~
     , gevoegd : gevoegde~          -- Persoon
     , "gemachtigde voor gevoegde" : (gevoegde~;door~/\inzake~);gemachtigde~
     , rechtsgebied : rechtsgebied
     , proceduresoort : proceduresoort
     , zitting : zaak~;zitting
             = [ kamer : kamer
               , geagendeerd : geagendeerd
               , rechter : rechter
               , griffier : griffier
               ]
     , behandeling : zaak~
        = [ rolnummer : I[Behandeling]
          , zittingsnummer : zitting
          , zaaknummer : zaak
          ]
     , bevoegd : bevoegd
     , "zorgdrager voor dossier" : zorgdrager           -- Orgaan
     , zaaksdossier : zaaksdossier~                     -- [Document]
        = [ Document : I[Document]
          , type : type
          ]
     , cluster : cluster
        = [ nr  : I[Cluster]
          , naam  : naam
          , grond : grond ]
     , machtigingen : inzake~
        = [ machtiging : I[Machtiging]
          , partij : door
          , gemachtigde : gemachtigde~
          ]
     , zittingen : zaak~
         = [ behandeling : zitting
               = [ zitting : I
                 , rechter : rechter
                 , locatie : locatie
                 , kamer : kamer
                 ]]
     ]

SERVICE Cluster : I[Cluster]
   = [ naam : naam
     , grond : grond
     , Procedure_of_cluster : cluster~
         = [ "zorgdrager voor dossier": zorgdrager
           , rechtsgebied : rechtsgebied
           , proceduresoort : proceduresoort]]

SERVICE Clusters : I[ONE]
   = [ Clusters : V[ONE*Cluster]
         = [ nr : I[Cluster]
           , naam : naam
           , zaken : cluster~
               = [ "zorgdrager voor dossier": zorgdrager
                 , rechtsgebied : rechtsgebied
                 , proceduresoort : proceduresoort
                 ]
           ]
     ]

  SERVICE Procedures : I[ONE]
   = [ Procedures : V[ONE*Procedur]
        = [ nr : I[Procedur]
          , zitting : zaak~;zitting
          , rechtsgebied : rechtsgebied
          , proceduresoort : proceduresoort
          , "zorgdrager voor dossier" : zorgdrager
          , gerecht : zaak~;zitting;locatie
          , clusters : cluster
             = [ zaken : cluster~ ]
          ]
     ]

SERVICE nieuweProcedure : I[Procedur]
   = [ "zorgdrager voor dossier" : zorgdrager
     , rechtsgebied : rechtsgebied
     , proceduresoort : proceduresoort]

  SERVICE Zitting : I[Zitting]
   = [ gerecht : locatie
     , kamer : kamer
     , plaats : plaats
     , rechter : rechter
     , griffier : griffier
     , geagendeerd : geagendeerd
     , feitelijkedatum : plaatsgevonden
     , zaken : (zaak~;zitting)~
        = [ zaaknr : I[Procedur]
          , eiser : eiser~
          , gedaagde : gedaagde~
          ]
     ]

  SERVICE Zittingen : I[ONE]
   = [ Zittingen : V[ONE*Zitting]
        = [ nr : I[Zitting]
          , kamer : kamer
          , gerecht : locatie
          , plaats : plaats
          , rechter : rechter
          , griffier : griffier
          , geagendeerd : geagendeerd
          , feitelijkedatum : plaatsgevonden
          ]
     ]

  SERVICE Plaats : I[Plaats]
   = [ Rechtbank : neven
     , ressort : neven;ressort
     , zittingen : plaats~
        = [ Zitting : I[Zitting]
          , rechter : rechter
          , geagendeerd : geagendeerd
          , kamer : kamer
          ]
     , "hoofdplaats" : hoofdplaats~
     ]

  SERVICE Partijen : I[ONE]
   = [ Eisers : V[ONE*Persoon];eiser;eiser~
     , Gedaagden : V[ONE*Persoon];gedaagde;gedaagde~
     , Gevoegden : V[ONE*Persoon];gevoegde;gevoegde~
     ]

  SERVICE Rolplanning : I[ONE]
   = [ Rol : V[ONE*Behandeling]
        = [ rolnr : I[Behandeling]
          , zaak : zaak
             = [ zaaknr   : I[Procedur]
               , gedaagde : gedaagde~
               , eiser    : eiser~
               , gevoegde : gevoegde~
               ]
          , zitting : zitting
             = [ kamer : kamer
               , rechter : rechter
               , griffier : griffier
               , geagendeerd : geagendeerd
               ]
          ]
     , Gepland : V[ONE*Behandeling]
        = [ rolnr : I
          , zitting : zitting
          , zaak : zaak
          ]
     ]

  SERVICE Behandeling : I[Behandeling]
   = [ zitting : zitting
        = [ gerecht : locatie
          , rechter : rechter
          , griffier : griffier
          , geagendeerd : geagendeerd
          , feitelijkedatum : plaatsgevonden
          ]
     , zaak : zaak
        = [ rechtsgebied : rechtsgebied
          , proceduresoort : proceduresoort
          , zitting : zaak~;zitting
          ]
     ]

  SERVICE ZaakInplannen : I[Behandeling]
   = [ zitting : zitting
     , zaak : zaak
     ]

  SERVICE Kamers : I[ONE]
   = [ Kamers : V[ONE*Kamer]
         = [ kamer : I
           , gerecht : gerecht
           , sector : sector
           , bezetting : bezetting~
           ]
     ]

  SERVICE Kamer : I[Kamer]
   = [ gerecht : gerecht
     , sector : sector
     , bezetting : bezetting~
     , zittingen : kamer~
     ]

  SERVICE Sector : I[Sector]
   = [ "sector Kamer" : sector~
        = [ Kamer : I[Kamer]
          , gerecht : gerecht
          ]
     ]

  SERVICE Gerecht : I[Gerecht]
   = [ Zittingen : locatie~
        = [ Zitting : I[Zitting]
          , kamer : kamer
          , rechter : rechter
          , griffier : griffier
          , geagendeerd : geagendeerd
          , rol : zitting~
             = [ nr : I[Behandeling]
               , zaak : zaak
               , proceduresoort : zaak;proceduresoort
               ]
          ]
     , kamers : gerecht~
     , benoemd : griffier \/ gerecht~;bezetting~
     , zaken : bevoegd~
     , hoofdplaats : hoofdplaats
     , nevenvestigingsplaatsen : neven~
     , teAgenderen : bevoegd~ /\ -(locatie~;zitting~;zaak) -- de rechtbank heeft kennis genomen van de zaak en de zaak staat nog niet op de rol...
             = [ zaaknr   : I[Procedur]
               , gedaagde : gedaagde~
               , eiser    : eiser~
               , gevoegde : gevoegde~
               ]
     ]

  SERVICE Artikel : I[Artikel]
   = [ tekst : wetstekst
     , "handeling" : handeling
     , "orgaan" : orgaan
     , "werkwoord" : werkwoord
     , "objecttype" : objecttype
     ]

  SERVICE Artikelen : I[ONE]
   = [ Artikelen : V[ONE*Artikel]
         = [ "artikel" : I[Artikel]
           , "handeling" : handeling
           , "orgaan" : orgaan
           , "werkwoord" : werkwoord
           , "objecttype" : objecttype
           ]
     ]


SERVICE RechterlijkeAmbtenaar : I[Persoon]
   = [ gerecht : griffier~ \/ bezetting;gerecht
     , kamer : bezetting
         = [ kamer : I[Kamer]
           , sector : sector
           ]
     , rol : vervult
     , "geautoriseerd voor" : aut
     , Zittingen : rechter~\/griffier[Zitting*Persoon]~
         = [ rechter : rechter
           , griffier : griffier[Zitting*Persoon]
           , geagendeerd : geagendeerd
           , plaats : plaats
           , locatie : locatie
           , kamer : kamer
               = [ gerecht : gerecht
                 , sector : sector]]
     , ontvangen : van~
         = [ bericht : I
           , van : van
           , verzonden : verzonden -- Tijdstip
           , ontvangen : verzonden] -- Tijdstip
     , verzonden : aan~
         = [ bericht : I
           , van : van
           , verzonden : verzonden]] -- Tijdstip

  SERVICE Personen : I[ONE]
   = [ Belanghebbenden : V[ONE*Persoon];(eiser;eiser~\/gedaagde;gedaagde~\/gevoegde;gevoegde~)
        = [ naam : I
          , rol : vervult ]
     , Gemachtigden : V[ONE*Persoon];gemachtigde;gemachtigde~
        = [ naam : I
          , rol : vervult ]
     , Rechters : V[ONE*Persoon];rechter~;rechter
        = [ naam : I
          , rol : vervult ]
     , Griffiers : V[ONE*Persoon];griffier~;I[Gerecht];griffier
        = [ naam : I
          , rol : vervult ]
     ]

  SERVICE Belanghebbenden : I[ONE]
-- Bug:   = [ Belanghebbenden : V[ONE*Persoon];(eiser\/gedaagde\/gevoegde);(eiser\/gedaagde\/gevoegde)~
   = [ Belanghebbenden : V[ONE*Persoon];(eiser;eiser~\/gedaagde;gedaagde~\/gevoegde;gevoegde~)
        = [ naam : I
          , rol : vervult
          , gemachtigde : door~;gemachtigde~
          ]
     ]

  SERVICE Gemachtigden : I[ONE]
   = [ Gemachtigden : V[ONE*Persoon];gemachtigde;gemachtigde~
        = [ naam : I , rol : vervult
          , vertegenwoordigt:gemachtigde;door
          , zaken : gemachtigde;inzake
          ]
     ]

  SERVICE RechterlijkeAmbtenaren : I[ONE]
   = [ Rechters : V[ONE*Persoon];bezetting;bezetting~
        = [ naam : I
          , gerecht : bezetting;gerecht
          , rol : vervult ]
     , Griffiers : V[ONE*Persoon];griffier~;I[Gerecht];griffier
        = [ naam : I
          , gerecht : griffier[Gerecht*Persoon]~
          , rol : vervult ]
     ]

  SERVICE Persoon : I[Persoon]
   = [ "procedure(s)" : eiser\/gedaagde\/gevoegde
         = [ nr : I[Procedur]
           , "zorgdrager voor dossier" : zorgdrager
           , rechtsgebied : rechtsgebied
           , proceduresoort : proceduresoort]
     , rol : vervult
     , "gemachtigde van" : door~
         = [ gemachtigde : gemachtigde~]
     , DigID : digid[DigID*Persoon]~
     , Dossier : van~ \/ aan~
         = [ van : van
           , aan : aan
           , verzonden : verzonden]
     ]

SERVICE Belanghebbende : I[Persoon]
   = [ "procedure(s)" : eiser\/gedaagde\/gevoegde
         = [ nr : I[Procedur]
           , "zorgdrager voor dossier" : zorgdrager
           , rechtsgebied : rechtsgebied
           , proceduresoort : proceduresoort]
     , Dossier : van~ \/ aan~
         = [ van : van
           , aan : aan
           , kenmerk : kenmerkVan
           , datum : verzonden]
     ]

  SERVICE Machtiging : I[Machtiging]
   = [ machtiging : I[Machtiging]
     , inzake : inzake
     , door : door
     , gemachtigde : gemachtigde~
     , "schriftelijke machtiging" : machtiging~
        = [ Document : I[Document]
          , type : type
          ]
     ]

  SERVICE Machtigingen : I[ONE]
   = [ Machtigingen : V[ONE*Machtiging]
        = [ stuk : I[Machtiging]
          , inzake : inzake
          , door : door
          , gemachtigde : gemachtigde~
          ]
     ]

  SERVICE Rechtsgebied : I[Rechtsgebied]
   = [ "Procedures" : rechtsgebied~
        = [ Procedure : I[Procedur]
          , proceduresoort : proceduresoort
          , "zorgdrager voor dossier": zorgdrager
          ]
     ]

  SERVICE Proceduresoort : I[Proceduresoort]
   = [ "Procedures" : proceduresoort~
        = [ Procedure : I[Procedur]
          , rechtsgebied : rechtsgebied
          , "zorgdrager voor dossier": zorgdrager
          ]
     ]

