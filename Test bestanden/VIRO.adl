  type :: Actie -> Handeling PRAGMA "" " is een instantie van type "
  EXPLANATION "Bijvoorbeeld: 'Piet de Vries is een poststuk aan het registreren.' is een actie van het type 'registreren van een poststuk'".
  type :: Ding * Objecttype PRAGMA "" " is een object van type "
  EXPLANATION "Bijvoorbeeld: poststuk 2009/87832 is een object van type Exploot".
  vervult :: Persoon * Rol PRAGMA "" " vervult "
  EXPLANATION "Bijvoorbeeld: Eida de Vries heeft de rol van beroepschriftbeoordelaar".

type[Actie*Handeling];handeling~;orgaan |- als[Actie*Orgaan]
  EXPLANATION "De persoon die een actie uitvoert doet dat als vertegenwoordiger van het orgaan dat de handeling uitvoert"

  prio :: Usecase*Moscow [UNI].
  sub :: Usecase*Usecase [ASY].
  use_case :: Usecase*Handeling.
  mag :: Handeling*Rol PRAGMA "" " mag worden uitgevoerd door "
  EXPLANATION "Om een handeling te mogen uitvoeren, is een rolpermissie benodigd. Bijvoorbeeld: Een secretaresse mag binnengekomen poststukken openen.".
  fase :: Usecase*Fase.
  categorie :: Usecase*Gpstap [UNI].
  omschrijving :: Usecase*Tekst [UNI].
  iS_component_objecttype :: Usecase*Component [UNI].
  opmerkingen :: Usecase*Tekst [UNI].
  form :: Usecase*FormCodes [UNI].
  bron :: Usecase*Referentie [UNI].
ENDPATTERN

PATTERN RBAC

  login :: Persoon*Sessie [SUR,INJ] PRAGMA "" " is ingelogd in ".
  digid :: Sessie -> DigID          PRAGMA "" " is geautoriseerd op basis van ".
  digid :: DigID -> Persoon         PRAGMA "" " verwijst rechtsgeldig naar ".

  login;digid |- digid~ EXPLANATION "Elke sessie behoort geautoriseerd te zijn op basis van de juiste DigID"
  login;rol |- vervult EXPLANATION "De gebruiker in deze sessie dient in te loggen met een van de rollen die hij of zij vervult."
  vervult |- aut EXPLANATION "Elke persoon die een rol vervult moet daarvoor geautoriseerd zijn."
  serviceRol :: BuService * Rol
      PRAGMA "" " mag worden aangeroepen door ".

  rol :: Sessie -> Rol PRAGMA "" " is ingelogd als ".
  aut :: Persoon * Rol PRAGMA "" " mag " " vervullen".
  start :: Sessie -> Tijdstip       PRAGMA "" " is ingelogd op ".
  einde :: Sessie * Tijdstip [UNI]  PRAGMA "" " is uitgelogd op ".
ENDPATTERN

SERVICE Sessie : I[Sessie]
   = [ persoon : login~
     , DigID : digid[Sessie*DigID]
     , startTijdstip : start
     , rol : rol
     ]

SERVICE Sessies : I[ONE]
   = [ Sessies : V[ONE*Sessie]
         = [ nr : I[Sessie]
           , start : start
           , einde : einde
           , DigID : digid[Sessie*DigID]
           , rol : rol
           , persoon : login~]]

