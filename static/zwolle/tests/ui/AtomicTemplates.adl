--REPRESENT Datatype TYPE INTEGER
--REPRESENT Datatype TYPE OBJECT
--REPRESENT Datatype TYPE PASSWORD
--REPRESENT Datatype TYPE TYPEOFONE
  
  RELATION r[A*Datatype] [UNI]
  RELATION s[A*Datatype]
  
  INTERFACE "Datatype" : V[SESSION*A] TABS
    [ "1cRud" : I BOX
      [ "uni" : r       cRud
      , "non-uni" : s   cRud
      , "s2" : s        cRud
      ]
    , "2crUd" : I BOX
      [ "uni" : r       crUd
      , "non-uni" : s   crUd
      , "s2" : s        cRud
      ]
    , "3cRUd" : I BOX
      [ "uni" : r       cRUd
      , "non-uni" : s   cRUd
      , "s2" : s        cRud
      ]
    , "4CRUD" : I BOX
      [ "uni" : r       CRUD
      , "non-uni" : s   CRUD
      , "s2" : s        cRud
      ]
    ]

ENDCONTEXT