CONTEXT "Test UI"

  CONCEPT A ""
  POPULATION A CONTAINS ["a1", "a2"]
  
  CONCEPT Datatype ""
--REPRESENT Datatype TYPE ALPHANUMERIC
--REPRESENT Datatype TYPE BIGALPHANUMERIC
--REPRESENT Datatype TYPE BOOLEAN
--REPRESENT Datatype TYPE DATE
--REPRESENT Datatype TYPE DATETIME
