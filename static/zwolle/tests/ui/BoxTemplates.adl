CONTEXT "Test UI"

  CONCEPT A ""
  POPULATION A CONTAINS ["a1", "a2"]
  
  CONCEPT B ""
  POPULATION B CONTAINS ["b1", "b2"]
  
  CONCEPT Datatype ""
  REPRESENT Datatype TYPE ALPHANUMERIC

  RELATION alpha[A*Datatype] [UNI]
  RELATION r[A*B] [UNI]
  RELATION s[A*B]
  
  INTERFACE "A" : I[A] cRud BOX
    [ "alpha" : alpha CRUD
    , "r" : r CRUD
    , "s" : s CRUD
    ]
  
--[ROOT interfaces that start with SESSION]----------------
  INTERFACE "1Aas-CRUD" : V[SESSION*A] CRUD COLS
    [ "I" : I
    , "alpha" : alpha
    , "r" : r
    , "s" : s
    ]
  INTERFACE "2Aas-cRud" : V[SESSION*A] cRud COLS
    [ "I" : I
    , "alpha" : alpha
    , "r" : r
    , "s" : s
    ]
  INTERFACE "3Aas-cRUd" : V[SESSION*A] cRUd COLS
    [ "I" : I
    , "alpha" : alpha
    , "r" : r
    , "s" : s
    ]
  INTERFACE "4AasCrud" : V[SESSION*A] CRUD COLS
    [ "I" : I
    , "alpha" : alpha
    , "r" : r
    , "s" : s
    ]

--[ROOT interfaces that start with UNI relation expression]----------------
  INTERFACE "1r-CRUD" : r CRUD COLS
    [ "I" : I
    ]
  INTERFACE "2r-cRud" : r cRud COLS
    [ "I" : I
    ]
  INTERFACE "3r-cRUd" : r cRUd COLS
    [ "I" : I
    ]
  INTERFACE "4rCrud" : r CRUD COLS
    [ "I" : I
    ]

--[ROOT interfaces that start with NON-UNI relation expression]----------------
  INTERFACE "1s-CRUD" : s CRUD COLS
    [ "I" : I
    ]
  INTERFACE "2s-cRud" : s cRud COLS
    [ "I" : I
    ]
  INTERFACE "3s-cRUd" : s cRUd COLS
    [ "I" : I
    ]
  INTERFACE "4sCrud" : s CRUD COLS
    [ "I" : I
    ]

ENDCONTEXT