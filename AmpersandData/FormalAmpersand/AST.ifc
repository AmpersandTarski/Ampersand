    , "reprType" : reprType[TblSQL*TType]
    ]       
INTERFACE SqlAttribute FOR Ampersand : I[SqlAttribute]
BOX [ "table" : table
    , "key~" : key~
    , "conceptAttribute~" : conceptAttribute~
    , "concept" : concept
    , "tgtAtt~" : tgtAtt~
    , "srcAtt~" : srcAtt~
    ]
-}
